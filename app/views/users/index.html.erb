
<h2>List of Users</h2>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  
  <tbody>

  <% @user.each do |user| %>

    <tr>   
      <td><%= link_to user.username, user_path(user.id) %></td>
      <td><%= user.votes.count %></td>
      <td><%= user.created_at %></td>
    </tr>

  <% end%>
    
  </tbody>
</table>